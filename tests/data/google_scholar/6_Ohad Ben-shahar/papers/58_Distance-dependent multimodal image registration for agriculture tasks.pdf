                  <mo>[</mo>
                  <mrow>
                    <mtable>
                      <mtr>
                        <mtd>
                          <mi>A</mi>
                        </mtd>
                        <mtd>
                          <mi>t</mi>
                        </mtd>
                      </mtr>
